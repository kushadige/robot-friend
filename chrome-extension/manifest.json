{
    "manifest_version" : 3,
    "name" : "Robot Friend",
    "description" : "Quick launch Social Media",
    "version" : "1.0.0",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["popup.js"]
        }
    ], 
    "action" : {
        "default_popup" : "popup.html",
        "default_title" : "Robot Friend"
    },
    "icons" : {"128" : "icon_128.png"},
    "permissions" : ["tabs"]
}